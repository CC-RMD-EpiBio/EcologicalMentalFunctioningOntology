<owl:Class rdf:about="https://github.com/CC-RMD-EpiBio/EcologicalMentalFunctioningOntology/EMFO#Throughput">
     <owl:equivalentClass>
     <owl:Restriction>
          <owl:onProperty rdf:resource="https://github.com/CC-RMD-EpiBio/EcologicalMentalFunctioningOntology/EMFO#part_of"/>
