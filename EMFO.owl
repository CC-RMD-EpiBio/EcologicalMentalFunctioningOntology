<owl:Class rdf:about="https://github.com/CC-RMD-EpiBio/EcologicalMentalFunctioningOntology/EMFO#Proximal_Contexts">
     <owl:equivalentClass>
     <owl:Restriction>
          <owl:onProperty rdf:resource="https://github.com/CC-RMD-EpiBio/EcologicalMentalFunctioningOntology/EMFO#part_of"/>
